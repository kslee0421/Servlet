<script>
	var html = document.getElementById("box");
	function aaa(){
		//{} 배열구조는 대표기가 있으며,단 중복되선 안된다.
		var data = {
		"myinfo": ["홍길동","25"," hong@nate.com"],
		"myinfo": ["김유신","30"," kim@nate.com"]
		}
		console.log(data);		
	}
	
	function bbb(){
		//대표키(Table 명) 대표키 안에 각각의 파트별로 보조키(컬럼멸) 을 
		//이용하요 배열을 설계하는 방식
		var data = {
			"myinfo":[
			{		"id ":"hong",
					"name ":"홍길동",
					"age" :"24"
			},
			{		"id ":"hong",
					"name ":"홍길동",
					"age" :"24"
			}	
			]		
		}	
		var ea = data["myinfo"].length;
		//console.log(ea);
		var w =0;
		while(w < ea){
			console.log(data["myinfo"][w]);
			w++;
		}
	}
	function ccc(){
		//대표키 (Table명) 이며, 그 외에 2차배열 형태로 데이터 값을순서대로 
		//나열하여 출력하는 배열형태
		var data = {
			"myinfo" :[
				["hong","25","hong@naver.com"],
				["kim","30","kim@nate.oom"]
			
			]
		}
		console.log(data);	 		
	}
	function ddd(){
		var data = {
				"myinfo":[
					{"id":"hong","name":"홍길동","age":"25",
					"log_history":[
						{"last_day":"2023-08-09"}
					]
					},
					{"id":"kim","name":"김유신","age":"30",
					"log_history":[
						{"last_day":""}
					]
					},
				]
		}
		console.log(data);
	}
	
</script>
</html>