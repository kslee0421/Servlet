<script>
	var html = document.getElementById("box");
	function aaa(){
		//{} 배열구조는 대표기가 있으며,단 중복되선 안된다.
		var data = {
		"myinfo": ["홍길동","25"," hong@nate.com"],
		"myinfo": ["김유신","30"," kim@nate.com"]
		}
		console.log(data);		
	}
	
	function bbb(){
		//대표키(Table 명) 대표키 안에 각각의 파트별로 보조키(컬럼멸) 을 
		//이용하요 배열을 설계하는 방식
		var data = {
			"myinfo":[
			{		"id ":"hong",
					"name ":"홍길동",
					"age" :"24"
			},
			{		"id ":"hong",
					"name ":"홍길동",
					"age" :"24"
			}	
			]		
		}	
		console.log(data);
	}
