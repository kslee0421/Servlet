<script>
	function abc(){
		//console.log("Test")
		var no1 = document.getElementById("no1");
		var no2 = document.getElementById("no2");
		var no3 = document.getElementById("no3");
		var no4 = document.getElementById("no4");
		var no5 = document.getElementById("no5");
		var no6 = document.getElementById("no6");		
		if(no1.value == ""){
			alert("첫번째 숫자 값을 입력하세요");
			no1.focus();
		}
		else if(no2.value ==""){
			alert("두번째 숫자 값을 입력하세요");
			no2.focus();			
		}
		else if(no3.value ==""){
			alert("세번째 숫자 값을 입력하세요");
			no3.focus();			
		}
		else if(no4.value ==""){
			alert("네번째 숫자 값을 입력하세요");
			no3.focus();			
		}
		else if(no5.value ==""){
			alert("다섯번째 숫자 값을 입력하세요");
			no4.focus();			
		}
		else if(no6.value ==""){
			alert("두번째 숫자 값을 입력하세요");
			no5.focus();			
		}
		else{
			f.submit();
		}
	}	
</script>
