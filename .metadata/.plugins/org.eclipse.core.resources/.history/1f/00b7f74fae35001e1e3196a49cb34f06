package web;
//Module 파트 Class로 만듬
public class index {
	//this 뭐뭐 써봤닝 1. 
	int sum = 0; // 필드에 선언하면 아해 this를 쓸 수 있다.
	public String test(String a, String b) {
		//valueOf : 주어진값이 숫자인지 글자인지 판별후 가지고옴 , parseInt : 문자를 숫자로 바꿔줌
		this.sum = Integer.valueOf(a) + Integer.parseInt(b);
		/*
		 * Integer.valueOf :  문자를 숫자로 변환시 값이 숫자인지 문자인지 검토 후 int로 변환을 설정 
		 * Integer.parseInt : 문자를 (int 형으로 변환)
		 * */
		System.out.println(this.sum);
		
		return null;
	}
	
}
